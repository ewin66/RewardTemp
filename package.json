{
  "name": "electron-react-typescript-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "gypfile": true,
  "main": "src/start.js",
  "build": {
    "appId": "Reward Panel",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "category": "your.app.category.type",
      "iconUrl": "/src/assets/logo.png"
    },
    "mac": {
      "category": "your.app.category.type",
      "iconUrl": "/src/assets/logo.png"
    }
  },
  "dependencies": {
    "electron": "^6.0.12",
    "electron-is-dev": "^1.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-overflow-scrolling": "^1.0.1",
    "react-remove-scroll-bar": "^2.0.0",
    "react-scripts": "3.2.0",
    "node-addon-api": "^0.6.3"
  },
  "scripts": {
    "electron": "electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-builder": "^21.2.0"
  }
}
